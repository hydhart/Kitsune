            trigger OnAfterGetRecord();
            begin
                EmployeeName := '';

                FOR i := 1 TO 50 DO
                  InitializePayrunArray();

                i := 1;

                EmployeeMaster.INIT;
                EmployeeMaster.RESET;
                IF EmployeeMaster.GET("No.") THEN BEGIN
                  EmployeeName := EmployeeMaster."Search Name";
                END;

                //Previous Pay Run
                PayElementMaster.INIT;
                PayElementMaster.RESET;
                IF PayElementMaster.FINDFIRST() THEN REPEAT
                  i := PayElementMaster."Payslip Sequence";
                  DIM_PayCode[i] := PayElementMaster."Pay Code";
                  DIM_PreviousAmount[i] := 0;
                  PreviousPayRun.INIT;
                  PreviousPayRun.RESET;
                  PreviousPayRun.SETRANGE("Pay Run No.", PreviousPayRunNo);
                  PreviousPayRun.SETRANGE("Special Pay No.", PreviousSpecialPayRunNo);
                  PreviousPayRun.SETRANGE("Branch Code", BranchCode);
                  PreviousPayRun.SETRANGE("Calendar Code", CalendarCode);
                  PreviousPayRun.SETRANGE("Statistics Group", StatisticsGroup);
                  PreviousPayRun.SETRANGE("Shift Code", ShiftCode);
                  PreviousPayRun.SETRANGE("Employee No.", "No.");
                  PreviousPayRun.SETRANGE("Pay Code", PayElementMaster."Pay Code");
                  IF PreviousPayRun.FINDFIRST() THEN REPEAT
                    DIM_PayCode[i] := CurrentPayRun."Pay Code";
                    DIM_PayDescription[i] := PayElementMaster."Pay Description";
                    DIM_PayType[i] := PayElementMaster."Pay Type";
                    DIM_PreviousAmount[i] := ROUND(PreviousPayRun.Amount, 0.01, '=');
                  UNTIL PreviousPayRun.NEXT = 0;
                UNTIL PayElementMaster.NEXT = 0;

                //Current Pay Run
                PayElementMaster.INIT;
                PayElementMaster.RESET;
                IF PayElementMaster.FINDFIRST() THEN REPEAT
                  i := PayElementMaster."Payslip Sequence";
                  CurrentPayRun.INIT;
                  CurrentPayRun.RESET;
                  CurrentPayRun.SETRANGE("Pay Run No.", CurrentPayRunNo);
                  CurrentPayRun.SETRANGE("Special Pay No.", CurrentSpecialPayRunNo);
                  CurrentPayRun.SETRANGE("Branch Code", BranchCode);
                  CurrentPayRun.SETRANGE("Calendar Code", CalendarCode);
                  CurrentPayRun.SETRANGE("Statistics Group", StatisticsGroup);
                  CurrentPayRun.SETRANGE("Shift Code", ShiftCode);
                  CurrentPayRun.SETRANGE("Employee No.", "No.");
                  CurrentPayRun.SETRANGE("Pay Code", PayElementMaster."Pay Code");
                  IF CurrentPayRun.FINDFIRST() THEN REPEAT
                    DIM_PayCode[i] := CurrentPayRun."Pay Code";
                    DIM_PayDescription[i] := PayElementMaster."Pay Description";
                    DIM_PayType[i] := PayElementMaster."Pay Type";
                    DIM_CurrentAmount[i] := ROUND(CurrentPayRun.Amount, 0.01, '=');
                  UNTIL CurrentPayRun.NEXT = 0;
                UNTIL PayElementMaster.NEXT = 0;
            end;
        }
